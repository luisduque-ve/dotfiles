[user]
  name = Luis Duque
  email = {{ .email | quote }}
[core]
  editor = vim
[merge]
  conflictstyle = diff3
[diff]
  colorMoved = default
[pull]
  rebase = true
[push]
  autoSetupRemote = true
[init]
  defaultBranch = main
[alias]
  cb = checkout -b
  st = status
[http]
  sslVerify = true
[rerere]
	enabled = true
