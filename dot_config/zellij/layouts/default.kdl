layout {
  tab name="main" {
    pane size=1 borderless=true {
      plugin location="tab-bar"
    }
    floating_panes {
      pane name="" {
        x 1
        y "10%"
        width 200
        height "50%"
      }
    }
    pane
    pane size=1 borderless=true {
      plugin location="status-bar"
    }
  }

  tab name="aux" {
    pane size=1 borderless=true {
      plugin location="tab-bar"
    }
    pane
    pane size=1 borderless=true {
      plugin location="status-bar"
    }
  }
}
